{
  "name": "lu-admin-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --fix --ignore-path .gitignore",
    "preview": "vite preview",
    "reInstall": "rimraf node_modules && pnpm install",
    "lint:eslint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:lint-staged": "lint-staged -c ./.husky/lint-staged.config.cjs",
    "lint:pretty": "pretty-quick --staged",
    "lint:prettier": "prettier --write \"src/**/*.{cjs,js,ts,json,tsx,jsx,css,scss,less,vue,html,md}\"",
    "lint:stylelint": "stylelint \"./**/*.{css,less,scss,vue,html}\" --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@ant-design/icons": "^5.1.4",
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@types/node": "^20.4.5",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "antd": "^5.7.3",
    "autoprefixer": "^10.4.14",
    "commitlint": "^17.6.7",
    "cssnano": "^6.0.1",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "husky": "^8.0.3",
    "immer": "^10.0.2",
    "lint-staged": "^13.2.3",
    "postcss": "^8.4.27",
    "postcss-html": "^1.5.0",
    "postcss-import": "^15.1.0",
    "postcss-scss": "^4.0.6",
    "prettier": "^3.0.0",
    "pretty-quick": "^3.1.3",
    "react-router-dom": "^6.14.2",
    "rimraf": "^5.0.1",
    "sass": "^1.64.1",
    "stylelint": "^15.10.2",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-order": "^6.0.3",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "zustand": "^4.3.9"
  },
  "description": "This template provides a minimal setup to get React working in Vite with HMR and some ESLint rules.",
  "main": "index.js",
  "author": "",
  "license": "ISC",
  "browserslist": [
    "> 1%",
    "not ie 11",
    "not op_mini all"
  ]
}
